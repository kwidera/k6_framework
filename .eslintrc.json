{
  "globals": {
      "__ENV": "readonly"
  },
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": ["eslint:recommended","plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "eol-last": ["error", "always"],
      "semi": ["error", "always"],
      "quotes": ["error", "single"]
  }
}
